<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fleet.startplan">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <!--    first screen-->
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StartPlan">
        <activity
            android:name=".Activity.IntroActivity" android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activity.MainActivity"    android:screenOrientation="portrait" android:exported="true"></activity>
        <activity android:name=".Activity.SettingActivity" android:screenOrientation="portrait" android:exported="true" ></activity>
        <activity android:name=".Activity.UpdateActivity" android:screenOrientation="portrait" android:exported="true"></activity>
        <activity android:name=".Activity.OpenSourceActivity" android:screenOrientation="portrait" android:exported="true" ></activity>
        <activity android:name=".Activity.PremiumActivity" android:screenOrientation="portrait" android:exported="true"></activity>
        <activity android:name=".Routine.RoutineActivity" android:screenOrientation="portrait"  android:exported="true"></activity>
        <activity android:name=".Bring.BringYesterday" android:screenOrientation="portrait" android:exported="true"></activity>
        <activity android:name=".Bring.BringTomorrow" android:screenOrientation="portrait" android:exported="true"></activity>
        <activity android:name=".Bring.BringCalendarPickDate" android:screenOrientation="portrait" android:exported="true"></activity>
        <activity android:name=".Bring.BringPickDate" android:screenOrientation="portrait" android:exported="true"></activity>
        <activity android:name=".Send.SendYesterday" android:screenOrientation="portrait" android:exported="true"></activity>
        <activity android:name=".Send.SendTomorrow" android:screenOrientation="portrait" android:exported="true"></activity>
        <activity android:name=".Send.SendCalendarPickDate" android:screenOrientation="portrait" android:exported="true"></activity>
        <activity android:name=".Send.SendPickDate" android:screenOrientation="portrait" android:exported="true"></activity>

        <activity android:name=".SuperFocus.SuperFocusActivity" android:screenOrientation="portrait" android:exported="true"></activity>
        <activity android:name=".SuperFocus.LockScreenActivity" android:screenOrientation="portrait" android:exported="true"></activity>
        <receiver
            android:name=".SuperFocus.ScreenReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.SCREEN_OFF" />
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
                <action android:name="android.intent.action.MY_PACKAGE_REPLACED"/>
            </intent-filter>
        </receiver>

        <service
            android:name=".SuperFocus.ScreenService"
            android:enabled="true"
            android:permission="android.permission.SYSTEM_ALERT_WINDOW"></service>


        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-5538450535019206~6701426870" />


        <service android:name=".FireBase.MyFireBaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>

    </application>

</manifest>