<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="71dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingStart="20dp"
    android:paddingEnd="20dp"
    android:paddingTop="7dp"
    >


    <androidx.constraintlayout.widget.ConstraintLayout

        android:layout_width="0dp"
        android:layout_height="64dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        >

        <View
            android:id="@+id/layout_item_schedule_todo"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/shape_schedule_item_container"
            />



        <View
        android:id="@+id/v_item_schedule_todo_emoji_space"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/shape_schedule_item_emoji"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginStart="17dp"
        />


        <TextView
            android:id="@+id/tv_item_schedule_todo_emoji"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="@+id/v_item_schedule_todo_emoji_space"
            app:layout_constraintEnd_toEndOf="@+id/v_item_schedule_todo_emoji_space"
            app:layout_constraintBottom_toBottomOf="@id/v_item_schedule_todo_emoji_space"
            app:layout_constraintTop_toTopOf="@+id/v_item_schedule_todo_emoji_space"
            />

        <TextView
            android:id="@+id/tv_item_schedule_todo_contents"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:textColor="@color/black"
            android:text="9급 준비"
            android:textStyle="bold"
            android:maxLines="1"
            android:ellipsize="end"
            android:layout_marginEnd="17dp"
            app:layout_constraintStart_toEndOf="@id/v_item_schedule_todo_emoji_space"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="17dp"
            />



    </androidx.constraintlayout.widget.ConstraintLayout>




</androidx.constraintlayout.widget.ConstraintLayout>